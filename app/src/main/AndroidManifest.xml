<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.tiregdev.atentik">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.splash_screen"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.login" />
        <activity android:name=".Activity.register" />
        <activity android:name=".Activity.home">
            <intent-filter>
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.edit_profile" />
        <activity android:name=".Activity.jadwal_kuliah" />
        <activity android:name=".Activity.login_dosen" />
        <activity android:name=".Activity.register_dosen" />
        <activity android:name=".Activity.statistik_view"/>
    </application>

</manifest>